#LEDBUTTON
	width 260px
	height 90px
	margin auto
	font-size 40px
	border-bottom 5px solid #F2777A
	text-align center
	display flex
	.text
		margin-left auto
		margin-right auto
		align-self flex-end

#IMU
	margin auto
	width 50%
	display flex
	flex-direction column
	font-size 40px
	.rowlike
		display flex
		flex-direction row
		text-align center
		width 100%
		& + .rowlike
			border-top 1px solid black
	.element
		width 100%
		& + .element
			border-left 1px solid black
